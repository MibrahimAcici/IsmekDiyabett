        android:background="#192E35"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="23:45"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tv_saat"
        app:layout_constraintTop_toBottomOf="@id/edt_gun"
