        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@id/edt_saat"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_gun"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="215dp" />

    <EditText
        android:id="@+id/edt_yil"
        android:layout_width="261dp"
        android:layout_height="29dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="25dp"
        android:background="#192E35"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="2022"
        android:textAlignment="textStart"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_yil"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <EditText
        android:id="@+id/edt_ay"
