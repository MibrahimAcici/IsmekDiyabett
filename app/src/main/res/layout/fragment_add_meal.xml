    </TextView>

    <EditText
        android:id="@+id/edt_6"
        android:layout_width="330dp"
        android:layout_height="29dp"
        android:layout_marginTop="20dp"
        android:background="#192E35"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
