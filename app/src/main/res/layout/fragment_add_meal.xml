<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="view.fragment.AddMealFragment">


    <View
        android:id="@+id/textView"
        android:layout_width="208dp"
        android:layout_height="40dp"
        android:background="@drawable/ic_yeni_ogun"
        android:gravity="center"

        app:layout_constraintBottom_toBottomOf="@id/edt_yil"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/tv_yil"
        android:layout_width="64dp"
        android:layout_height="29dp"
        android:layout_margin="12dp"
        android:background="#477768"
        android:gravity="center"
        android:text="YIL"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@id/edt_yil"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/tv_ay"
        android:layout_width="64dp"
        android:layout_height="29dp"
        android:layout_margin="12dp"
